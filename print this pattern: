#include <stdio.h>

int main() {
    int rows;
    int columns;
    printf("Enter row : ");
    scanf("%d",&rows);
    printf("Enter columns : ");
    scanf("%d",&columns);

    // printing star patterns

    for(int i = 1 ; i <= rows ; i++) {
        for(int j = 1 ; j <= i ; j++){
            printf("*");
        }
        printf("\n");
    }

    //printing square

    for(int i = 1 ; i <= rows ; i++) {
        for(int j = 1 ; j <= columns ; j++){
            printf("*");
        }
        printf("\n");
    }

    return 0;

}
